{
  "compilerOptions": {
    "target": "ESNext", 
    "module": "CommonJS", 
    "moduleResolution": "Node", 
    "lib": ["ESNext", "DOM"], 
    "strict": true, 
    "skipLibCheck": true, 
    "noEmit": true, 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "resolveJsonModule": true, 
    "allowSyntheticDefaultImports": true, 
    "jsx": "react-jsx", 
    "jsxImportSource": "hono/jsx" 
  },
  "include": ["src"], 
  "exclude": ["node_modules", "dist"] 
}
